<div class=container>
  <div class="col-md-6 col-md-offset-3 col-sm-10">
    <%= form_for(@picture) do |f| %>
      <%= f.file_field :picture %>
      <%= f.hidden_field :picture_cache %>

      <%= f.label :description %>
      <%= f.text_field :description, class: "form-control" %>

      <%= f.submit "投稿", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
